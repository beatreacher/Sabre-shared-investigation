<?xml version="1.0" encoding="utf-8"?>
<schema xmlns:eb="http://services.sabre.com/sp/exchange/booking/v1" xmlns:spct="http://services.sabre.com/sp/common/simple/v3" xmlns:ebct="http://services.sabre.com/sp/exchange/booking/common/types/v1" xmlns:stlp="http://services.sabre.com/STL_Payload/v02_01" xmlns:xsd="http://services.sabre.com/STL_MessageCommon/v02_01" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://services.sabre.com/sp/exchange/booking/v1" xmlns="http://www.w3.org/2001/XMLSchema">
  <import schemaLocation="built-ins/STL2_MsgCommon_v02_01.xsd" namespace="http://services.sabre.com/STL_MessageCommon/v02_01" />
  <import schemaLocation="built-ins/STL2_Payload_v02_01.xsd" namespace="http://services.sabre.com/STL_Payload/v02_01" />
  <import schemaLocation="ExchangeBookingCommonTypes1.0.0.xsd" namespace="http://services.sabre.com/sp/exchange/booking/common/types/v1" />
  <import schemaLocation="SpCommonSimpleTypes_v3.xsd" namespace="http://services.sabre.com/sp/common/simple/v3" />
  <element name="ExchangeBookingRQ">
    <annotation>
      <documentation>Exchange Booking Request</documentation>
    </annotation>
    <complexType>
      <complexContent mixed="false">
        <extension base="stlp:STL_Request_Payload">
          <sequence>
            <element name="Itinerary" type="eb:ItineraryType">
              <annotation>
                <documentation>Itinerary</documentation>
              </annotation>
            </element>
            <element minOccurs="0" name="Cancel" type="eb:CancelType">
              <annotation>
                <documentation>Cancel</documentation>
              </annotation>
            </element>
            <element minOccurs="0" name="AirBook" type="eb:AirBookType">
              <annotation>
                <documentation>AirBook</documentation>
              </annotation>
            </element>
            <element minOccurs="0" maxOccurs="30" name="CreditVerificationRQ" type="eb:CreditVerificationRQType">
              <annotation>
                <documentation>CreditVerificationRQ</documentation>
              </annotation>
            </element>
            <element maxOccurs="9" name="AutomatedExchanges" type="eb:AutomatedExchangesType">
              <annotation>
                <documentation>AutomatedExchanges</documentation>
              </annotation>
            </element>
            <element minOccurs="0" name="MiscSegmentSell" type="eb:MiscSegmentSellType">
              <annotation>
                <documentation>MiscSegmentSell</documentation>
              </annotation>
            </element>
            <element minOccurs="0" name="SpecialService" type="eb:SpecialServiceType">
              <annotation>
                <documentation>SpecialService</documentation>
              </annotation>
            </element>
            <element minOccurs="0" name="DeleteAccountingLines" type="eb:DeleteAccountingLinesType">
              <annotation>
                <documentation>DeleteAccountingLines</documentation>
              </annotation>
            </element>
            <element minOccurs="0" name="DeletePriceQuote" type="eb:DeletePriceQuoteType">
              <annotation>
                <documentation>DeletePriceQuote</documentation>
              </annotation>
            </element>
            <element name="PostProcessing" type="eb:PostProcessingType">
              <annotation>
                <documentation>PostProcessing</documentation>
              </annotation>
            </element>
          </sequence>
          <attribute default="false" name="healthCheck" type="string" use="optional">
            <annotation>
              <documentation>Health Check</documentation>
            </annotation>
          </attribute>
          <attribute name="targetCity" type="string" use="optional">
            <annotation>
              <documentation>"TargetCity" is used to specify to which city service should change context
                                using ContextChange (AAA) and the rest of the service uses new security. If empty or
                                equals current city the context doesn't change.
                            </documentation>
            </annotation>
          </attribute>
        </extension>
      </complexContent>
    </complexType>
  </element>
  <complexType name="ItineraryType">
    <annotation>
      <documentation>ItineraryType</documentation>
    </annotation>
    <sequence>
      <element minOccurs="0" name="SegmentPricing">
        <annotation>
          <documentation>
                        "SegmentPricing" is used to instruct the system to price specific segments during exchange
                        comparison step. If the PNR is to remain unchanged after Cancel and AirBook steps, please
                        specify all existing segments as the ones to be priced.
                    </documentation>
        </annotation>
        <complexType>
          <sequence>
            <element maxOccurs="16" name="SegmentSelect">
              <annotation>
                <documentation>SegmentSelect</documentation>
              </annotation>
              <complexType>
                <attribute name="number" type="integer" use="required">
                  <annotation>
                    <documentation>number attribute name</documentation>
                  </annotation>
                </attribute>
              </complexType>
            </element>
          </sequence>
        </complexType>
      </element>
    </sequence>
    <attribute name="id" type="string" use="required">
      <annotation>
        <documentation>"id" is used to specify a record locator.</documentation>
      </annotation>
    </attribute>
  </complexType>
  <complexType name="CancelType">
    <annotation>
      <documentation>CancelType</documentation>
    </annotation>
    <sequence>
      <element maxOccurs="unbounded" name="Segment">
        <annotation>
          <documentation>Segment</documentation>
        </annotation>
        <complexType>
          <attribute name="Number" type="eb:CancelSegmentNumber" use="required">
            <annotation>
              <documentation>
                                "Number" is used to specify a particular segment number to cancel.
                            </documentation>
            </annotation>
          </attribute>
        </complexType>
      </element>
    </sequence>
  </complexType>
  <complexType name="AirBookType">
    <annotation>
      <documentation>AirBookType</documentation>
    </annotation>
    <sequence>
      <element minOccurs="0" maxOccurs="unbounded" name="HaltOnStatus">
        <annotation>
          <documentation>Halt on Status</documentation>
        </annotation>
        <complexType>
          <attribute name="Code" use="required">
            <annotation>
              <documentation>
                                "Code" is used to specify values that cause subsequent pricing, and beyond processing to
                                stop if they appear. In order to successfully price an air itinerary the status code
                                associated with the individual segments cannot be "NN," "NO," "UC," or "US." After
                                successfully booking flights, the system will compare the status codes returned from the
                                carrier against the client-specified codes to determine if subsequent pricing, and
                                beyond processing should occur.
                            </documentation>
            </annotation>
            <simpleType>
              <restriction base="string">
                <minLength value="1" />
              </restriction>
            </simpleType>
          </attribute>
        </complexType>
      </element>
      <element name="OriginDestinationInformation">
        <annotation>
          <documentation>
                        "ArrivalDateTime" is used to specify the time and date of flight arrival. Please note that the
                        value within this attribute if passed cannot be null.
                    </documentation>
        </annotation>
        <complexType>
          <sequence>
            <element maxOccurs="10" name="FlightSegment">
              <annotation>
                <documentation>Flight Segment</documentation>
              </annotation>
              <complexType>
                <sequence>
                  <element name="DestinationLocation">
                    <annotation>
                      <documentation>DestinationLocation</documentation>
                    </annotation>
                    <complexType>
                      <attribute name="LocationCode" type="string" use="required">
                        <annotation>
                          <documentation>
                                                        "LocationCode" is used to specify the arrival airport code.
                                                    </documentation>
                        </annotation>
                      </attribute>
                    </complexType>
                  </element>
                  <element minOccurs="0" name="Equipment">
                    <annotation>
                      <documentation>Equipment</documentation>
                    </annotation>
                    <complexType>
                      <attribute name="AirEquipType" type="string" use="required">
                        <annotation>
                          <documentation>
                                                        "AirEquipType" is used to specify the equipment type of the
                                                        aircraft being booked.
                                                    </documentation>
                        </annotation>
                      </attribute>
                    </complexType>
                  </element>
                  <element name="MarketingAirline">
                    <annotation>
                      <documentation>MarketingAirline</documentation>
                    </annotation>
                    <complexType>
                      <attribute name="Code" type="string" use="required">
                        <annotation>
                          <documentation>
                                                        "Code" is used to specify the marketing airline code.
                                                    </documentation>
                        </annotation>
                      </attribute>
                      <attribute name="FlightNumber" type="string" use="required">
                        <annotation>
                          <documentation>
                                                        "FlightNumber" is used to specify the flight number. OPEN can
                                                        also be passed if the user desires an open space ticket.
                                                    </documentation>
                        </annotation>
                      </attribute>
                    </complexType>
                  </element>
                  <element minOccurs="0" name="MarriageGrp">
                    <annotation>
                      <documentation>
                                                Information whether the flight segment is part of a married connection.
                                                "O" means "no". "I" means "yes"
                                            </documentation>
                    </annotation>
                    <simpleType>
                      <restriction base="string">
                        <enumeration value="O" />
                        <enumeration value="I" />
                      </restriction>
                    </simpleType>
                  </element>
                  <element minOccurs="0" name="OperatingAirline">
                    <annotation>
                      <documentation>OperatingAirline</documentation>
                    </annotation>
                    <complexType>
                      <attribute name="Code" type="string" use="required">
                        <annotation>
                          <documentation>
                                                        "Code" is used to specify the operating airline code.
                                                    </documentation>
                        </annotation>
                      </attribute>
                    </complexType>
                  </element>
                  <element name="OriginLocation">
                    <annotation>
                      <documentation>OriginLocation</documentation>
                    </annotation>
                    <complexType>
                      <attribute name="LocationCode" type="string" use="required">
                        <annotation>
                          <documentation>
                                                        "LocationCode" is used to specify the departure airport code.
                                                    </documentation>
                        </annotation>
                      </attribute>
                    </complexType>
                  </element>
                </sequence>
                <attribute name="ArrivalDateTime" type="spct:dateTime" use="optional">
                  <annotation>
                    <documentation>
                                            "ArrivalDateTime" is used to specify the time and date of flight arrival.
                                            Please note that the value within this attribute if passed cannot be null.
                                        </documentation>
                  </annotation>
                </attribute>
                <attribute name="DepartureDateTime" type="spct:dateTime" use="optional">
                  <annotation>
                    <documentation>
                                            "DepartureDateTime" is used to specify the time and date of flight
                                            departure.
                                        </documentation>
                  </annotation>
                </attribute>
                <attribute name="FlightNumber" type="string" use="required">
                  <annotation>
                    <documentation>
                                            "FlightNumber" is used to specify the flight number. OPEN can also be passed
                                            if the user desires an open space ticket.
                                        </documentation>
                  </annotation>
                </attribute>
                <attribute name="NumberInParty" type="string" use="required">
                  <annotation>
                    <documentation>
                                            "NumberInParty" is used to specify the number of passengers that need to be
                                            booked during this transaction.
                                        </documentation>
                  </annotation>
                </attribute>
                <attribute name="ResBookDesigCode" type="string" use="required">
                  <annotation>
                    <documentation>
                                            "ResBookDesigCode" is used to specify the booking class.
                                        </documentation>
                  </annotation>
                </attribute>
                <attribute name="Status" type="string" use="required">
                  <annotation>
                    <documentation>
                                            "Status" is used to specify the action code to be used to sell the flight
                                            inventory. Valid values for "Status": NN, HL, GK, QF, SA, MM, or OPEN.
                                        </documentation>
                  </annotation>
                </attribute>
                <attribute name="InstantPurchase" type="boolean" use="optional">
                  <annotation>
                    <documentation>
                                            "InstantPurchase" is equal to "true" EnhancedAirBook will call OTA_AirBookRQ
                                            and if it is equal to "false" EnhancedAirBook will call OTA_AirBookLLSRQ.
                                        </documentation>
                  </annotation>
                </attribute>
              </complexType>
            </element>
          </sequence>
        </complexType>
      </element>
    </sequence>
  </complexType>
  <complexType name="CreditVerificationRQType">
    <annotation>
      <documentation>CreditVerificationRQType</documentation>
    </annotation>
    <sequence>
      <element minOccurs="0" name="Credit">
        <annotation>
          <documentation>Credit</documentation>
        </annotation>
        <complexType>
          <sequence>
            <element minOccurs="0" name="CC_Info">
              <annotation>
                <documentation>CC_Info</documentation>
              </annotation>
              <complexType>
                <sequence>
                  <element name="PaymentCard">
                    <annotation>
                      <documentation>PaymentCard</documentation>
                    </annotation>
                    <complexType>
                      <attribute name="AirlineCode" type="string" use="required">
                        <annotation>
                          <documentation>
                                                        "AirlineCode" is used to specify the airline code.
                                                    </documentation>
                        </annotation>
                      </attribute>
                      <attribute name="CardSecurityCode" type="string" use="optional">
                        <annotation>
                          <documentation>
                                                        "CardSecurityCode" is used to specify a credit card security
                                                        code.
                                                    </documentation>
                        </annotation>
                      </attribute>
                      <attribute name="Code" type="string" use="required">
                        <annotation>
                          <documentation>
                                                        "Code" is used to specify the credit card vendor code.
                                                    </documentation>
                        </annotation>
                      </attribute>
                      <attribute name="ExpireDate" type="spct:expireDate" use="required">
                        <annotation>
                          <documentation>
                                                        "ExpireDate" is to specify the credit card expiration date.
                                                    </documentation>
                        </annotation>
                      </attribute>
                      <attribute name="ExtendedPayment" type="integer" use="optional">
                        <annotation>
                          <documentation>
                                                        "ExtendedPayment" is used to specify extended payment if
                                                        applicable This qualifier is only available to BSP Mexico
                                                        clients.
                                                    </documentation>
                        </annotation>
                      </attribute>
                      <attribute name="Number" type="integer" use="required">
                        <annotation>
                          <documentation>
                                                        "Number" is used to specify the credit card number.
                                                    </documentation>
                        </annotation>
                      </attribute>
                    </complexType>
                  </element>
                </sequence>
              </complexType>
            </element>
            <element minOccurs="0" name="ItinTotalFare">
              <annotation>
                <documentation>ItinTotalFare</documentation>
              </annotation>
              <complexType>
                <sequence>
                  <element name="TotalFare">
                    <annotation>
                      <documentation>TotalFare</documentation>
                    </annotation>
                    <complexType>
                      <attribute name="Amount" type="string" use="required">
                        <annotation>
                          <documentation>
                                                        "Amount" is used to specify the total fare amount.
                                                    </documentation>
                        </annotation>
                      </attribute>
                      <attribute name="CurrencyCode" type="string" use="required">
                        <annotation>
                          <documentation>
                                                        "CurrencyCode" is used to specify the currency code of the total
                                                        fare amount.
                                                    </documentation>
                        </annotation>
                      </attribute>
                    </complexType>
                  </element>
                </sequence>
              </complexType>
            </element>
          </sequence>
          <attribute name="AutoStore" type="boolean" use="optional">
            <annotation>
              <documentation>
                                "AutoStore" is used to auto store the manual approval code within the PNR.
                            </documentation>
            </annotation>
          </attribute>
          <attribute name="DisplayReference" type="boolean" use="optional">
            <annotation>
              <documentation>
                                "DisplayReference" is used to display Payment Web Service Reference number if available.
                            </documentation>
            </annotation>
          </attribute>
          <attribute name="RemarkNumber" type="integer" use="optional">
            <annotation>
              <documentation>
                                "RemarkNumber" is used to request a "Tasa Administrativa" approval code for Colombian
                                agents and refer it to the corresponding payment remarks. It must used together with
                                .../Credit/@TasaAdministrative
                            </documentation>
            </annotation>
          </attribute>
          <attribute name="ServiceFee" type="boolean" use="optional">
            <annotation>
              <documentation>
                                "ServiceFee" is used to indicate that authorization amount is for service fee amount for
                                Argentinian agents.
                            </documentation>
            </annotation>
          </attribute>
          <attribute name="TasaAdministrativa" type="boolean" use="optional">
            <annotation>
              <documentation>
                                "TasaAdministrativa" is used to request a "Tasa Administrativa" approval code for
                                Colombian agents.
                            </documentation>
            </annotation>
          </attribute>
        </complexType>
      </element>
      <element minOccurs="0" name="DescriptiveBilling">
        <annotation>
          <documentation>
                        "DescriptiveBilling" is used to Create Amex or TP Airplus DBI with Linear Format.
                    </documentation>
        </annotation>
        <complexType>
          <sequence>
            <element minOccurs="0" name="CC_Info">
              <annotation>
                <documentation>CC_Info</documentation>
              </annotation>
              <complexType>
                <sequence>
                  <element name="PaymentCard">
                    <annotation>
                      <documentation>PaymentCard</documentation>
                    </annotation>
                    <complexType>
                      <attribute name="Code" type="string" use="required">
                        <annotation>
                          <documentation>
                                                        "Code" is used to specify the credit card vendor code. Currently
                                                        system will accept only "AX" or "TP".
                                                    </documentation>
                        </annotation>
                      </attribute>
                      <attribute name="NameNumber" type="string" use="optional">
                        <annotation>
                          <documentation>
                                                        "NameNumber" is used to associate the DBI data with a passenger.
                                                        If the request is applicable to all passengers input "ALL".
                                                    </documentation>
                        </annotation>
                      </attribute>
                      <attribute name="Number" type="string" use="optional">
                        <annotation>
                          <documentation>
                                                        "Number" is used to specify the credit card number.
                                                    </documentation>
                        </annotation>
                      </attribute>
                    </complexType>
                  </element>
                  <element minOccurs="0" name="FreeText">
                    <annotation>
                      <documentation>
                                                "FreeText" is used to enter data elements.
                                            </documentation>
                    </annotation>
                    <complexType>
                      <sequence>
                        <element minOccurs="0" name="Amex">
                          <annotation>
                            <documentation>Amex element name</documentation>
                          </annotation>
                          <complexType>
                            <sequence>
                              <element minOccurs="0" name="R1" type="string">
                                <annotation>
                                  <documentation>R1 element name</documentation>
                                </annotation>
                              </element>
                              <element minOccurs="0" name="R2" type="string">
                                <annotation>
                                  <documentation>R2 element name</documentation>
                                </annotation>
                              </element>
                              <element minOccurs="0" name="R3" type="string">
                                <annotation>
                                  <documentation>R3 element name</documentation>
                                </annotation>
                              </element>
                              <element minOccurs="0" name="R4" type="string">
                                <annotation>
                                  <documentation>R4 element name</documentation>
                                </annotation>
                              </element>
                              <element minOccurs="0" name="R5" type="string">
                                <annotation>
                                  <documentation>R5 element name</documentation>
                                </annotation>
                              </element>
                              <element minOccurs="0" name="R6" type="string">
                                <annotation>
                                  <documentation>R6 element name</documentation>
                                </annotation>
                              </element>
                              <element minOccurs="0" name="OI" type="string">
                                <annotation>
                                  <documentation>
                                                                        "OI" is used to enter Original Invoice
                                                                        information. Maximum 10 Alphanumeric characters
                                                                        are allowed.
                                                                    </documentation>
                                </annotation>
                              </element>
                              <element minOccurs="0" name="Vat" type="string">
                                <annotation>
                                  <documentation>
                                                                        "Vat" is used to enter VAT information. Maximum
                                                                        8 Numeric characters are allowed.
                                                                    </documentation>
                                </annotation>
                              </element>
                            </sequence>
                          </complexType>
                        </element>
                        <element minOccurs="0" name="Airplus">
                          <annotation>
                            <documentation>Airplus</documentation>
                          </annotation>
                          <complexType>
                            <sequence>
                              <element minOccurs="0" name="PK" type="string">
                                <annotation>
                                  <documentation>
                                                                        "PK" is used to specify Personal ID
                                                                    </documentation>
                                </annotation>
                              </element>
                              <element minOccurs="0" name="KS" type="string">
                                <annotation>
                                  <documentation>
                                                                        "KS" is used to enter cost account number.
                                                                        Maximum 10 Alphanumeric characters are allowed.
                                                                    </documentation>
                                </annotation>
                              </element>
                              <element minOccurs="0" name="IK" type="string">
                                <annotation>
                                  <documentation>
                                                                        "IK" is used to enter account number.Maximum 1
                                                                        Alphanumeric character is allowed.
                                                                    </documentation>
                                </annotation>
                              </element>
                              <element minOccurs="0" name="PR" type="string">
                                <annotation>
                                  <documentation>
                                                                        "PR" is used to enter project number. Maximum 14
                                                                        Alphanumeric characters are allowed.
                                                                    </documentation>
                                </annotation>
                              </element>
                              <element minOccurs="0" name="AK" type="string">
                                <annotation>
                                  <documentation>
                                                                        "AK" is used to enter action code.
                                                                    </documentation>
                                </annotation>
                              </element>
                              <element minOccurs="0" name="DS" type="string">
                                <annotation>
                                  <documentation>
                                                                        "DS" is used to enter department code. Maximum
                                                                        10 Alphanumeric characters are allowed.
                                                                    </documentation>
                                </annotation>
                              </element>
                              <element minOccurs="0" name="AE" type="string">
                                <annotation>
                                  <documentation>
                                                                        "AE" is used to enter accounting unit. Maximum 8
                                                                        Alphanumeric characters are allowed.
                                                                    </documentation>
                                </annotation>
                              </element>
                              <element minOccurs="0" name="BD" type="string">
                                <annotation>
                                  <documentation>
                                                                        "BD" is used to enter file date information.
                                                                    </documentation>
                                </annotation>
                              </element>
                              <element minOccurs="0" name="RZ" type="string">
                                <annotation>
                                  <documentation>
                                                                        "RZ" is used to enter destination information.
                                                                    </documentation>
                                </annotation>
                              </element>
                              <element minOccurs="0" name="AU" type="string">
                                <annotation>
                                  <documentation>
                                                                        "AU" is used to enter order number information.
                                                                    </documentation>
                                </annotation>
                              </element>
                            </sequence>
                          </complexType>
                        </element>
                      </sequence>
                    </complexType>
                  </element>
                </sequence>
                <attribute name="Add" type="boolean" use="optional">
                  <annotation>
                    <documentation>
                                            "Add" is used to add the DBI information using the linear format.
                                        </documentation>
                  </annotation>
                </attribute>
                <attribute name="Delete" type="boolean" use="optional">
                  <annotation>
                    <documentation>
                                            "Delete" is used to delete the DBI information using the linear format.
                                        </documentation>
                  </annotation>
                </attribute>
                <attribute name="Modify" type="boolean" use="optional">
                  <annotation>
                    <documentation>
                                            "Modify" is used to modify the DBI information using the linear format.
                                        </documentation>
                  </annotation>
                </attribute>
              </complexType>
            </element>
          </sequence>
          <attribute name="Display" type="boolean" use="optional">
            <annotation>
              <documentation>
                                "Display" is used to display DBI information. It is not to be used with any elements.
                            </documentation>
            </annotation>
          </attribute>
        </complexType>
      </element>
    </sequence>
  </complexType>
  <complexType name="AutomatedExchangesType">
    <annotation>
      <documentation>AutomatedExchangesType</documentation>
    </annotation>
    <sequence>
      <element minOccurs="0" name="ExchangeComparison">
        <annotation>
          <documentation>
                        The "ExchangeComparison" node is used to derive the price of the exchange.
                    </documentation>
        </annotation>
        <complexType>
          <sequence>
            <element minOccurs="0" name="ExchangeOverrides">
              <annotation>
                <documentation>ExchangeOverrides</documentation>
              </annotation>
              <complexType>
                <sequence>
                  <element minOccurs="0" name="ChangeFee" type="string">
                    <annotation>
                      <documentation>ChangeFee</documentation>
                    </annotation>
                  </element>
                  <element minOccurs="0" name="FeeOnPenalty">
                    <annotation>
                      <documentation>FeeOnPenalty</documentation>
                    </annotation>
                    <complexType>
                      <sequence>
                        <element maxOccurs="2" name="Tax">
                          <annotation>
                            <documentation>Tax element name</documentation>
                          </annotation>
                          <complexType>
                            <attribute name="Amount" type="string" use="required">
                              <annotation>
                                <documentation>Amount</documentation>
                              </annotation>
                            </attribute>
                            <attribute name="Code" type="string" use="required">
                              <annotation>
                                <documentation>Code attribute name</documentation>
                              </annotation>
                            </attribute>
                          </complexType>
                        </element>
                      </sequence>
                    </complexType>
                  </element>
                  <element minOccurs="0" name="TicketFeeAmount" type="string">
                    <annotation>
                      <documentation>TicketFeeAmount</documentation>
                    </annotation>
                  </element>
                </sequence>
              </complexType>
            </element>
            <element name="PriceRequestInformation">
              <annotation>
                <documentation>PriceRequestInformation</documentation>
              </annotation>
              <complexType>
                <sequence>
                  <element name="OptionalQualifiers">
                    <annotation>
                      <documentation>OptionalQualifiers</documentation>
                    </annotation>
                    <complexType>
                      <sequence>
                        <element minOccurs="0" name="FlightQualifiers">
                          <annotation>
                            <documentation>FlightQualifiers</documentation>
                          </annotation>
                          <complexType>
                            <sequence>
                              <element name="VendorPrefs">
                                <annotation>
                                  <documentation>VendorPrefs</documentation>
                                </annotation>
                                <complexType>
                                  <sequence>
